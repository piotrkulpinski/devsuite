    "@types/he": "^1.2.3",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@vercel/remix": "^2.12.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "prisma": "^5.19.1",
    "tailwindcss": "^3.4.12",
