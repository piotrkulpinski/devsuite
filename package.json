{
  "name": "devsuite",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "bun biome lint --apply .",
    "check": "bun biome check --apply .",
    "format": "bun biome format --organize-imports-enabled --write .",
    "typecheck": "tsc",
    "db:generate": "prisma generate --no-hints",
    "db:studio": "prisma studio",
    "db:push": "prisma db push --skip-generate",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --skip-seed && prisma db push --skip-generate && prisma db seed",
    "postinstall": "bun run db:generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.61",
    "@curiousleaf/utils": "^1.0.29",
