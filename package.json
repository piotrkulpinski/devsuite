{
  "name": "devsuite",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev --name ",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push --skip-generate",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --skip-seed && prisma db push --skip-generate && prisma db seed",
    "postinstall": "bun run db:generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.31",
    "@curiousleaf/utils": "^1.0.27",
    "@epic-web/remember": "^1.0.2",
    "@hookform/resolvers": "^3.4.2",
    "@prisma/client": "^5.15.1",
    "@radix-ui/react-checkbox": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/serve": "^2.9.2",
    "@sindresorhus/slugify": "^2.2.1",
    "@tiptap/pm": "^2.4.0",
    "@tiptap/react": "^2.4.0",
    "@tiptap/starter-kit": "^2.4.0",
    "ai": "^3.2.0",
    "cva": "^1.0.0-beta.1",
    "embla-carousel-react": "^8.1.5",
    "got": "^14.4.1",
    "he": "^1.2.0",
    "hotkeys-js": "^3.13.7",
    "isbot": "^4.1.0",
    "lucide-react": "^0.378.0",
    "next-themes": "^0.3.0",
    "node-html-parser": "^6.1.13",
    "plur": "^5.1.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "remix-hook-form": "^5.0.1",
    "remix-utils": "^7.6.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.2",
